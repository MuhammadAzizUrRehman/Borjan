var datalist = document.getElementById("cities");
var input = document.getElementById("cityInput");
var targetedValue = document.getElementById("TextField5");
var selectedCity = '';

citiesListUnsort.forEach(function(city) {
  var option = document.createElement("option");
  option.value = city;
  datalist.appendChild(option);
});

input.addEventListener('input', function() {
  selectedCity = this.value;
  localStorage.setItem("selectedCity", selectedCity);
});


function cityDropdownValidation() {   
  var storedSelectedCity = localStorage.getItem("selectedCity");

    
  if (input.value.length > 0 && storedSelectedCity === input.value) {
    setTimeout(function() {
      window.location.href = `/checkout?checkout[shipping_address][city]=${storedSelectedCity}`;
    }, 500);
  }else {
    document.querySelector(".cart-error-message").style.display = "block";
    document.querySelector(".cart-error-message").style.color = "red";
    
  }

}


